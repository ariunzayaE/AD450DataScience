grade = float(input("Enter your grade: "))
if grade >= 95.0:
    print("Your GPA is 4.0")
elif grade < 95.0 and grade >= 94.0:
    print("Your GPA is 3.9")
elif grade < 94.0 and grade >= 93.0:
    print("Your GPA is 3.8")
elif grade < 93.0 and grade >= 92.0:
    print("Your GPA is 3.7")
elif grade < 92.0 and grade >= 91.0:
    print("Your GPA is 3.6")
elif grade < 91.0 and grade >= 90.0:
    print("Your GPA is 3.5")
elif grade < 90.0 and grade >= 89.0:
    print("Your GPA is 3.4")
elif grade < 89.0 and grade >= 88.0:
    print("Your GPA is 3.3")
elif grade < 88.0 and grade >= 87.0:
    print("Your GPA is 3.2")
elif grade < 87.0 and grade >= 86.0:
    print("Your GPA is 3.1")
elif grade < 86.0 and grade >= 85.0:
    print("Your GPA is 3.0")
elif grade < 85.0 and grade >= 84.0:
    print("Your GPA is 2.9")
elif grade < 84.0 and grade >= 83.0:
    print("Your GPA is 2.8")
elif grade < 83.0 and grade >= 82.0:
    print("Your GPA is 2.7")
elif grade < 82.0 and grade >= 81.0:
    print("Your GPA is 2.6")
elif grade < 81.0 and grade >= 80.0:
    print("Your GPA is 2.5")
elif grade < 80.0 and grade >= 79.0:
    print("Your GPA is 2.4")
elif grade < 79.0 and grade >= 78.0:
    print("Your GPA is 2.3")
elif grade < 78.0 and grade >= 77.0:
    print("Your GPA is 2.2")
elif grade < 77.0 and grade >= 76.0:
    print("Your GPA is 2.1")
elif grade < 76.0 and grade >= 75.0:
    print("Your GPA is 2.0")
elif grade < 75.0 and grade >= 74.0:
    print("Your GPA is 1.9")
elif grade < 74.0 and grade >= 73.0:
    print("Your GPA is 1.8")
elif grade < 73.0 and grade >= 72.0:
    print("Your GPA is 1.7")
elif grade < 72.0 and grade >= 71.0:
    print("Your GPA is 1.6")
elif grade < 71.0 and grade >= 70.0:
    print("Your GPA is 1.5")
elif grade < 70.0 and grade >= 69.0:
    print("Your GPA is 1.4")
elif grade < 69.0 and grade >= 68.0:
    print("Your GPA is 1.3")
elif grade < 68.0 and grade >= 67.0:
    print("Your GPA is 1.2")
elif grade < 67.0 and grade >= 66.0:
    print("Your GPA is 1.1")
elif grade < 66.0 and grade >= 65.0:
    print("Your GPA is 1.0")
else: 
    print("You are failed and your GPA is 0.0")
